{
  "build": {
    "target": "dev",
  },
  "dockerFile": "Dockerfile",
  "extensions": [
    "ms-vscode.go",
    "prisma.vscode-graphql",
    "visualstudioexptteam.vscodeintellicode",
  ],
  "mounts": [
    "type=bind,source=${localWorkspaceFolder},target=/boateng-api",
  ],
  "runArgs": [
    "--name=boateng-api-dev-container",
  ],
  "settings": {
    "go.gopath": "/go",
  },
  
  "name": "boateng-api-dev-container",
}